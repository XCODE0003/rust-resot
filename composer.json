{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "garetjax/phpbrowscap": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "heleket/php-sdk": "^1.0",
    "iignatov/lightopenid": "^1.0",
    "josiasmontag/laravel-recaptchav3": "^0.6.0",
    "laravel/framework": "^8.40",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.3",
    "pagseguro/pagseguro-php-sdk": "^6.0",
    "paymentwall/paymentwall-php": "^2.2.3",
    "paypal/rest-api-sdk-php": "^1.14",
    "phrity/net-uri": "^1.0",
    "phrity/util-errorhandler": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0 | ^2.0 | ^3.0",
    "qiwi/bill-payments-php-sdk": "^0.2.2",
    "textalk/websocket": "^1.6",
    "timehunter/laravel-google-recaptcha-v2": "~1.0.0",
    "whichbrowser/parser": "^2.1"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "files": [
      "app/Helpers.php",
      "app/Services/Qrcode.php"
    ],
    "classmap": [
      "app/Services/Qrcode.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "PayPal\\": "vendor/paypal/rest-api-sdk-php/lib/",
      "WebSocket\\": "vendor/websocket-php-1.6.3/lib/",
      "Phrity\\": "vendor/phrity-net-uri-main/src/Phrity/",
      "Qiwi\\Api\\": "vendor/qiwi/bill-payments-php-sdk/src/",
      "PagSeguro\\": "vendor/pagseguro/pagseguro-sdk-php-master/source/",
      "Paymentwall\\": "vendor/paymentwall/paymentwall-php/lib/Paymentwall/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "exclude-from-classmap": [
      "app/Services/Qrcode.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "platform": {
      "php": "7.4.33"
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
